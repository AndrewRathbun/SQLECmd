Description: Google Chrome History Visits
Author: Andrew Rathbun
Email: andrew.rathbun@kroll.com
Id: 1f37cc07-2b10-4c80-b845-84708e4f7429
Version: 1.0
CSVPrefix: GoogleChrome
FileName: History
IdentifyQuery: SELECT count(*) FROM sqlite_master WHERE type='table' AND (name='urls' OR name='visits' OR name='downloads' OR name='segments' OR name='typed_url_sync_metadata');
IdentifyValue: 5
Queries:
    -
        Name: History
        Query: |
                SELECT
                urls.id AS ID,
                datetime( urls.last_visit_time / 1000000 + ( strftime( '%s', '1601-01-01' ) ), 'unixepoch', 'localtime' ) AS LastVisitedTime,
                urls.url AS URL,
                urls.title AS URLTitle,
                urls.visit_count AS VisitCount,
                urls.typed_count AS TypedCount,
                CASE

                WHEN urls.hidden = 1 THEN
                'Yes'
                WHEN urls.hidden = 0 THEN
                'No'
                END AS Hidden
                FROM
                urls
                ORDER BY
                urls.last_visit_time ASC;
        BaseFileName: Visits

